    <flow id="flow_emergency_sw" type="rescue" route="route_sw" begin="0" end="100000" probability="0.001" departSpeed="max" departPos="base" departLane="best"/>

    <flow id="flow_ns" route="route_ns" begin="0" end="100000" departSpeed="max" departPos="base" departLane="best" probability="0.05"/>
